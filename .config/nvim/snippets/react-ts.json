{
	"React Component with Interface": {
		"prefix": "rci",
		"scope": "typescriptreact",
		"body": [
			"import { cn } from \"@/shared/lib/cn\";",
			"import { HTMLAttributes } from \"react\";",
			"",
			"export interface ${1:ComponentName}Props extends HTMLAttributes<HTMLDivElement> {",
			"}",
			"",
			"export const ${1:ComponentName} = ({ className, ...props }: ${1:ComponentName}Props) => {",
			"  return <div className={cn(\"\", className)} {...props}></div>;",
			"};"
		],
		"description": "React Component with custom Props interface and cn utility"
	},
